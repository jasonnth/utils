<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TypefaceTextView">
        <attr name="customFont" format="string" />
    </declare-styleable>

    <declare-styleable name="JNColoredRatingBar">
        <attr name="indicator" format="boolean" />
        <attr name="rating" format="float" />
    </declare-styleable>

    <declare-styleable name="JNTextInputView">
        <attr name="titleColor" format="integer" />
        <attr name="title" format="string" />
        <attr name="hint" format="string" />
        <attr name="weight" format="float" />
        <attr name="inputType" format="enum">
            <enum name="phone" value="0" />
            <enum name="email" value="1" />
            <enum name="number" value="2" />
            <enum name="password" value="3" />
            <enum name="normal" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="JNImageView">
        <attr name="src" format="integer" />
        <attr name="isCircle" format="boolean" />
    </declare-styleable>

    <declare-styleable name="JNTabItemView">
        <attr name="itemIcon" format="reference"/>
        <attr name="iconRight" format="reference"/>
        <attr name="text" format="string"/>
        <attr name="textRight"  format="string"/>
        <attr name="hideDivider"  format="boolean"/>
    </declare-styleable>
</resources>